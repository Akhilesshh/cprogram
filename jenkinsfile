steps {
sh '''
pwd
echo "$akhilesh"
'''
}
}
stage ('TEST') {
steps {
sh '''
echo "this is test stage"
'''
}
}
stage ('DEPLOY') {
steps {
sh '''
echo "this is deploy stage"
'''
}
}
stage ('4th') {
steps {
sh '''
pwd
'''
}
}
stage('5th') {
steps {
sh '''
echo "$$"
'''
}
}
}
}
